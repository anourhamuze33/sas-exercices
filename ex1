#include <stdio.h>
int main(void)
 {
  
     int i;
     int j;

 for(i=1; i<=6; i++){
   for(j=1; j<=9-i;j++){
     printf(" ");
     if(i==4&& j==1 || i==5 &&  j==2){
          printf("*");
     }
   }
 
    for(j=1;j<=9; j++){
     printf("*");
     
     if(i!=9-i+4){
         break;
     }
     
   }
   printf("\n");
 }
 return 0;
 }